{
  "name": "angular-nest-ssr",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:prod": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run angular-nest-ssr:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build && ng run angular-nest-ssr:server",
    "prerender": "ng run angular-nest-ssr:prerender",
    "vercel-build": "npm run prerender && npm run minify-html && npm run purge-css",
    "prebuild:ssr": "ngcc",
    "lint": "npm run lint:styles && ng lint --fix",
    "lint:styles": "npx stylelint src/**/*.scss --fix",
    "format": "npx prettier --log-level warn ./**/*.{html,scss,json,js,ts} --write",
    "minify-html": "npx ts-node-dev --transpile-only scripts/minifyHtml.ts",
    "purge-css": "npx purgecss -css dist/browser/*.css --content dist/browser/index.html dist/browser/*.js -o dist/browser/",
    "install:clean": "rm -rf node_modules/ && npm cache clean --force && npm ci",
    "prepare": "husky install"
  },
  "private": true,
  "engines": {
    "node": "^16.14.0 || ^18.10.0"
  },
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/platform-server": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/ng-universal": "^8.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nguniversal/express-engine": "^16.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "domino": "^2.1.6",
    "express": "^4.15.2",
    "http-status-codes": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.3",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "~16.2.3",
    "@angular/compiler-cli": "^16.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@node-minify/core": "^8.0.6",
    "@node-minify/html-minifier": "^8.0.6",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.1.1",
    "purgecss": "^5.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.1.3"
  }
}
